{
  "name": "ngx-carousel",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "rimraf dist",
    "ng": "ng",
    "start": "npm run prebuild && npm run lib:build  && concurrently --raw  \"npm run lib:start\" \"ng serve\" \"gulp copy-assets:watch\"",
    "build": "npm run prebuild && npm run lib:build:prod && ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:start": "ng build ngx-carousel --watch",
    "lib:build": "npm run prebuild && ng build ngx-carousel && gulp copy-assets",
    "lib:build:prod": "npm run prebuild && ng build ngx-carousel --configuration production && gulp copy-assets"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/localize": "15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "bootstrap": "5.1.3",
    "rxjs": "~6.5.4",
    "tslib": "^2.6.0",
    "zone.js": "~0.13"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "4.1.1",
    "copyfiles": "2.1.1",
    "gulp": "4.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.2.2",
    "protractor": "~7.0.0",
    "rimraf": "2.6.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9"
  }
}
