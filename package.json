{
  "name": "ngx-carousel",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "rimraf dist",
    "ng": "ng",
    "start": "npm run prebuild && npm run lib:build  && concurrently --raw  \"npm run lib:start\" \"ng serve\" \"gulp copy-assets:watch\"",
    "build": "npm run prebuild && npm run lib:build:prod && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:start": "ng build ngx-carousel --watch",
    "lib:build": "npm run prebuild && ng build ngx-carousel && gulp copy-assets",
    "lib:build:prod": "npm run prebuild && ng build ngx-carousel --prod && gulp copy-assets"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.5",
    "@angular/common": "~10.2.5",
    "@angular/compiler": "~10.2.5",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~10.2.5",
    "@angular/localize": "10.2.5",
    "@angular/platform-browser": "~10.2.5",
    "@angular/platform-browser-dynamic": "~10.2.5",
    "@angular/router": "~10.2.5",
    "bootstrap": "5.1.3",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.4",
    "@angular-devkit/build-ng-packagr": "~0.1002.4",
    "@angular/cli": "~10.2.4",
    "@angular/compiler-cli": "~10.2.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "4.1.1",
    "copyfiles": "2.1.1",
    "gulp": "4.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "rimraf": "2.6.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8"
  }
}
